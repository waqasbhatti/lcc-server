{
    "lc_formatkey": "hat-k2lc",
    "lc_fileglob": "*-k2lc.csv.gz",
    "lc_readermodule": "astrobase.hatsurveys.k2hat",
    "lc_readerfunc": "read_csv_lightcurve",
    "lc_readerfunc_kwargs": null,
    "lc_normalizemodule": null,
    "lc_normalizefunc": null,
    "lc_normalizefunc_kwargs": null,
    "lc_measurements_flux_or_mag": "mag",
    "unaffiliated_cols": [
        "bjd",
        "bgv",
        "bge",
        "frn",
        "xcc",
        "ycc",
        "arc"
    ],
    "mag_apertures":[
        "00",
